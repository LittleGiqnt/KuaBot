{
    "name": "prismbot",
    "description": "A bot for Prism discord community server.",
    "author": "hswhsw610",
    "scripts": {
        "start": "cross-env NODE_ENV=production NODE_PATH=./build node build/index.js",
        "build": "npx swc ./src -d build",
        "dev": "cross-env NODE_ENV=development NODE_PATH=./src nodemon --delay 0 --ext ts --watch ./src --exec \"node -r @swc-node/register ./src/index.ts\""
    },
    "devDependencies": {
        "@swc-node/register": "^1.5.4",
        "@swc/cli": "^0.1.57",
        "@swc/core": "^1.3.17",
        "@types/node": "^18.11.9",
        "@typescript-eslint/eslint-plugin": "^5.43.0",
        "@typescript-eslint/parser": "^5.43.0",
        "cross-env": "^7.0.3",
        "eslint": "^8.27.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-import": "^2.26.0",
        "eslint-plugin-n": "^15.5.1",
        "eslint-plugin-promise": "^6.1.1",
        "nodemon": "^2.0.20",
        "tsconfig-paths": "^4.1.0",
        "typescript": "^4.9.3"
    },
    "dependencies": {
        "axios": "^1.1.3",
        "discord.js": "^14.6.0",
        "discord.js-selfbot-v13": "^2.9.0",
        "dotenv": "^16.0.3",
        "mongoose": "^6.7.2"
    }
}