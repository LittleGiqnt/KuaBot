{
    "name": "prismbot",
    "description": "A bot for Prism discord community server.",
    "author": "hswhsw610",
    "scripts": {
        "start": "cross-env NODE_ENV=production NODE_PATH=./dist node dist/index.js",
        "build": "swc ./src -d dist",
        "dev": "concurrently -n W: \"npm:watch-*\"",
        "watch-swc": "swc ./src -d dist --watch",
        "watch-node": "cross-env NODE_ENV=development NODE_PATH=./dist nodemon --watch dist --watch locales dist/index.js"
    },
    "devDependencies": {
        "@swc-node/register": "^1.5.4",
        "@swc/cli": "^0.1.57",
        "@swc/core": "^1.3.24",
        "@types/node": "^18.11.17",
        "@types/node-schedule": "^2.1.0",
        "@types/sharp": "^0.31.0",
        "@typescript-eslint/eslint-plugin": "^5.47.0",
        "@typescript-eslint/parser": "^5.47.0",
        "chokidar": "^3.5.3",
        "concurrently": "^7.6.0",
        "cross-env": "^7.0.3",
        "eslint": "^8.30.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-import": "^2.26.0",
        "eslint-plugin-n": "^15.6.0",
        "eslint-plugin-promise": "^6.1.1",
        "mongodb-memory-server": "^8.10.2",
        "nodemon": "^2.0.20",
        "tsconfig-paths": "^4.1.1",
        "typescript": "^4.9.4"
    },
    "dependencies": {
        "axios": "^1.2.1",
        "bufferutil": "^4.0.7",
        "discord.js": "^14.7.1",
        "discord.js-selfbot-v13": "^2.9.16",
        "dotenv": "^16.0.3",
        "i18next": "^22.4.6",
        "i18next-fs-backend": "^2.1.1",
        "mongoose": "^6.8.1",
        "node-schedule": "^2.1.0",
        "utf-8-validate": "^5.0.10"
    }
}